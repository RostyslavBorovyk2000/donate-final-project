{
  "name": "donate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@cloudinary/react": "^1.11.2",
    "@cloudinary/url-gen": "^1.11.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.5.1",
    "formik": "^2.4.5",
    "lodash-es": "^4.17.21",
    "npm-run-all": "^4.1.1",
    "npm-watch": "^0.3.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-loader-spinner": "^5.4.5",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.16.0",
    "react-scripts": "5.0.1",
    "react-slick": "^0.29.0",
    "react-slider": "^2.0.6",
    "redux": "^4.2.1",
    "redux-devtools-extension": "^2.13.9",
    "slick-carousel": "^1.8.1",
    "stylelint": "^15.11.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-order": "^6.0.3",
    "web-vitals": "^2.1.4",
    "yup": "^1.3.2"
  },
  "scripts": {
    "start": "npm-run-all -p watch-sass-js start-js",
    "build": "npm run eslint && npm run stylelint-build && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "stylelint-dev": "stylelint src/**/*.scss --cache --cache-location './.stylelintcache' || true",
    "stylelint-build": "stylelint src/**/*.scss",
    "watch-sass-js": "npm-watch",
    "start-js": "react-scripts start",
    "stylelint-fix-sass": "stylelint --fix src/**/*.scss",
    "eslint": "eslint src/**/*.js"
  },
  "watch": {
    "stylelint-dev": {
      "patterns": [
        "/src/**/*.scss"
      ],
      "extensions": "scss"
    },
    "eslint": {
      "patterns": [
        "/src/**/*.js"
      ],
      "extensions": "js"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^8.51.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-react": "^7.33.2",
    "sass": "^1.68.0",
    "scss": "^0.2.4",
    "standard": "^17.1.0"
  }
}
